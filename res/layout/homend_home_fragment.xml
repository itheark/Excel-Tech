<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/homeback"
    android:orientation="vertical" >


    <ImageView
        android:id="@+id/register_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView2"
        android:layout_toLeftOf="@+id/navigate_icon"
        android:layout_marginTop="30dp"
        android:layout_marginRight="50dp"
        android:src="@drawable/ic_home_register"
        android:clickable="true" />

    
    <ImageView
        android:id="@+id/navigate_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/register_icon"
        android:layout_centerHorizontal="true"
        android:src="@drawable/ic_home_navigation"
        android:clickable="true" />

    <ImageView
        android:id="@+id/fb_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/navigate_icon"
        android:layout_alignTop="@+id/navigate_icon"
        android:layout_marginLeft="50dp"
        android:src="@drawable/ic_home_fb"
        android:clickable="true" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/collegemec" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@drawable/logohome" />

    <ImageView
        android:id="@+id/excelmecorg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/navigate_icon"
        android:layout_centerHorizontal="true"
        android:src="@drawable/excelmecorg" />

    <ImageView
        android:id="@+id/npbuilder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="20dp"
        android:layout_alignTop="@+id/imageView1"
        android:src="@drawable/nucleus" />
   
</RelativeLayout>
